chmod +x build_all
chmod +x test_all
chmod +x load_shell
chmod +x lib/buildlibs
rm -rf bin
mkdir bin
cd p2asm_src
chmod +x buildit
./buildit
cp p2asm.exe ../bin
cd ..
cd p2link_src
gcc p2link.c -o p2link
cp p2link.exe ../bin
cd ..
cd loadp2_src
chmod +x build_mingw
./build_mingw
cp loadp2.exe ../bin
cd ..
cd s2pasm_src
gcc s2pasm.c lmmsubs.c -o s2pasm
cp s2pasm.exe ../bin
cd ..
cd util
gcc p2dump.c -o p2dump
cp p2dump.exe ../bin
gcc p2dumpobj.c -o p2dumpobj
cp p2dumpobj.exe ../bin
cp diffit ../bin
cd ..
cp p2gcc bin
cp p2gcc.bat bin
